        <div class="panel-group" id="accordion">
          <div class="panel panel-default">
            <div class="panel-heading"> <span class="trs-icons-home" role="presentation"></span><a data-toggle="collapse" data-parent="#accordion" href="#collapseOne"> Home</a> </div>
            <div id="collapseOne" class="panel-collapse collapse in">
              <div class="panel-body">
                <table class="table">
                  <tr>
                    <td><span></span><a href="#">Overview</a></td>
                  </tr>
                  <tr class="active">
                    <td><span></span><a href="#">Balances</a></td>
                  </tr>
                  <tr>
                    <td><span></span><a href="#">Beneficiaries</a></td>
                  </tr>
                  <tr>
                    <td><span></span><a href="#">Forms</a></td>
                  </tr>
                  <tr>
                    <td><span></span><a href="#">E-Documents</a></td>
                  </tr>
                  <tr>
                    <td><span></span><a href="#">Account Summary</a></td>
                  </tr>
                </table>
              </div>
            </div>
          </div>
          <div class="panel panel-default">
            <div class="panel-heading"> <span class="trs-icons-eye" role="presentation"></span><a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">Review</a> </div>
            <div id="collapseTwo" class="panel-collapse collapse">
              <div class="panel-body">
                <table class="table">
                  <tr>
                    <td><span class="trs-icons-hamburger"></span><a href="#">Child</a></td>
                  </tr>
                </table>
              </div>
            </div>
          </div>
          <div class="panel panel-default">
            <div class="panel-heading"> <span class="trs-icons-settings" role="presentation"></span><a data-toggle="collapse" data-parent="#accordion" href="#collapseThree">Manage</a> </div>
            <div id="collapseThree" class="panel-collapse collapse">
              <div class="panel-body">
                <table class="table">
                  <tr>
                    <td><span class="trs-icons-hamburger"></span><a href="#">Child</a></td>
                  </tr>
                </table>
              </div>
            </div>
          </div>
          <div class="panel panel-default">
            <div class="panel-heading"> <span class="trs-icons-on-track" role="presentation"></span><a data-toggle="collapse" data-parent="#accordion" href="#collapseFour">Are You Ontrack</a> </div>
            <div id="collapseFour" class="panel-collapse collapse">
              <div class="panel-body">
                <table class="table">
                  <tr>
                    <td><span class="trs-icons-hamburger"></span><a href="#">Child</a></td>
                  </tr>
                </table>
              </div>
            </div>
          </div>
          <div class="panel-heading"> <span class="trs-icons-phone-ring contact" role="presentation"></span> <a href="#">Contact Us</a>
            <div class="phone">1-800-755-5801</div>
          </div>
        </div>
      </div>
    </div>
    <!-- /#sidebar-wrapper -->

    <!-- inner-body -->
    <div class="inner-body">
    <a href="#test" class="btn-primary btn-xs" id="menu-toggle">Toggle Menu</a>
      <h1>Select an Account Below to Continue</h1>
      {{# tables}}
      {{> organisms-select-account }}
      {{/ tables}}
      </div>
    <!-- inner-body -->

    <!-- Footer -->
    {{> organisms-trs-footer }}
    <!-- Footer -->

  </div>
  <!-- /#body-wrapper -->
</div>
<!-- /#page -->

<!-- Menu Toggle Script & menu height -->
<script>
$( document ).ready(function() {
    $("#sidebar-wrapper").height($("#body-wrapper").height());
});
$("#menu-toggle").click(function(e) {
	e.preventDefault();
	$("#body-wrapper").toggleClass("toggled");
});
</script>
