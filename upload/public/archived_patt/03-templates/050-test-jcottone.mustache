<style>
/* TEST FOR NEW LAYOUT */
.row {
	margin-right: 0px;
	margin-left: 0px;
}
.container {
	padding-right: 0px;
	padding-left: 0px;
}
header {
	height: auto;
}
#body-wrapper {
	width: 980px;
	padding: 0;
	overflow-y: hidden;
	-webkit-transition: all 0.5s ease;
	-moz-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
}
.inner-body {
	padding-left: 15px;
	padding-right: 15px;
}
.table .active {
	border-left: #e22739 solid 7px;
}
#sidebar-wrapper {
	top: 0px;
	left: 0px;
	width: 221px;
	float: left;
	height: 100%;
	overflow:hidden;
	border-right: #eeeeee solid 1px;
	margin-left: -221px;
	overflow-y: hidden;
	-webkit-transition: all 0.5s ease;
	-moz-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
}

@media(min-width:768px) {
#body-wrapper {
	padding-left: 221px;
}
#body-wrapper.toggled {
	padding-left: 0;
}
}
.panel-default .panel-heading {
	color: #333;
	background-color: #fff;
	border-color: #ddd;
	background-image: none;
	padding-left: 20px;
}
.panel-default .panel-heading span {
	padding-left: 15px;
}
.panel-default .panel-heading a {
	padding-left: 5px;
}
.panel-body {
	background-color: #f9f9f9;
	border-top: none;
	border-bottom: none;
}
.panel-body a:hover {
	text-decoration: none;
}
.panel-body .table tbody tr td {
	border: 0;
	padding-left: 40px;
}
.panel-group .panel-heading .panel-collapse .panel-body {
	border-top: none;
	border-bottom: none;
}
.panel-collapse {
	border-top: none;
}
.panel-heading .contact, .phone {
	padding-left: 20px;
	padding-top: 10px;
	padding-bottom: 10px;
	font-weight: bold;
}
.panel-heading .contact a {
	margin-left: 5px;
}
.panel-heading a {
	font-weight: bold;
}
.panel-heading a:hover {
	text-decoration: none;
}
.glyphicon {
	margin-right: 10px;
}
.panel-body {
	padding: 0px;
}
.panel-body table tr td {
	padding-left: 15px
}
.panel-body .table {
	margin-bottom: 0px;
}
</style>

<div id="page" class="container">
  <header class="container"> <a class="ada-only" href="#main_content">Skip to main content</a><!-- keep this for ada compliance -->
    <!-- Page Header -->
    <div class="container no-padding"> {{> organisms-header }} </div>
