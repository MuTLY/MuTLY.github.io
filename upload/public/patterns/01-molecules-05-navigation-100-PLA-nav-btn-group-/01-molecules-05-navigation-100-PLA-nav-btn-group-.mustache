&lt;script type='text/javascript'&gt;
$(function() {
    var slideHeight = 97;
    $(&quot;.pla-btn-group-container&quot;).each(function() {
        var $this = $(this);
        var $wrap = $this.children(&quot;.pla-btn-group-wrap&quot;);
        var defHeight = $wrap.height();
        if (defHeight &gt;= slideHeight) {
            var $viewMore = $this.find(&quot;.pla-view-more&quot;);
            $wrap.css(&quot;height&quot;, slideHeight + &quot;px&quot;);
            $viewMore.append(&quot;&lt;a href='#'&gt;View Full Menu&lt;/a&gt;&quot;);
            $viewMore.children(&quot;a&quot;).bind(&quot;click&quot;, function(event) {
                var curHeight = $wrap.height();
                if (curHeight &lt;= slideHeight) {

                    $wrap.animate({
                        height: defHeight
                    }, &quot;normal&quot;);
                    $(this).text(&quot;Hide Full Menu&quot;);

                } else {

                    $wrap.animate({
                        height: slideHeight
                    }, &quot;normal&quot;);
                    $(this).text(&quot;View Full Menu&quot;);

                }
                return false;
            });
        }
    });
});
&lt;/script&gt;
