&lt;script type=&quot;text/javascript&quot;&gt;
$(document).ready(function () {
    var msg;

    $.validator.addMethod(&quot;greaterThanZero&quot;, function (value, element) {
        valid = parseInt(value) != 'NaN' &amp;&amp; parseInt(value) &gt; 0;
        if (!valid) {
            //get the label for the element
            $label = $('label[for=&quot;' + $(element).attr('id') + '&quot;]');
            msg = &quot;Please enter a valid number greater than 0 &quot;
            if ($label.length &gt; 0) msg += &quot;for &quot; + $label.html();
        }
        return valid;
    }, function () {
        return msg;
    });

    $.validator.addMethod(&quot;retirementAgeGreaterThanAge&quot;, function (value) {
        return parseInt(value) != 'NaN' &amp;&amp; parseInt(value) &gt; 38;
    }, 'Your Desired Retirement Age cannot be less than your current age (&lt;strong&gt;38&lt;/strong&gt;).');

    $.validator.addMethod(&quot;retirementAgeLessThanLifeExpectancy&quot;, function (value) {
        var gender = $('input[name=gender]:checked').val();
        var lifeExpectancy = 85;
        if (gender == &quot;F&quot;) {
            lifeExpectancy = 88;
        }
        return parseInt(value) != 'NaN' &amp;&amp; parseInt(value) &lt; lifeExpectancy;
    }, 'Your Desired Retirement Age can not exceed your life expectancy');

    $('#newUserForm').validate({
        rules: {
            retirementAge: {
                required: true,
                retirementAgeGreaterThanAge: true,
                retirementAgeLessThanLifeExpectancy: true
            },
            salary: {
                required: true
            },
            preTaxContrib: {
                greaterThanZero: true
            },
            postTaxContrib: {
                greaterThanZero: true
            },
            rothContrib: {
                greaterThanZero: true
            }
        },
        messages: {
            retirementAge: {
                required: &quot;Please enter your desired retirement age.&quot;
            },
            salary: {
                required: &quot;Please enter your Current Annual Salary and try again.&quot;,
                pattern: &quot;Current Annual Salary has invalid characters, please correct and try again&quot;
            }
        },
        highlight: function (element) {
            var id_attr = &quot;#&quot; + $(element).attr(&quot;id&quot;) + &quot;1&quot;;
            $(element).closest('.form-group').removeClass('has-feedback success').addClass('has-feedback error');
            $(id_attr).removeClass('glyphicon-ok').addClass('glyphicon-remove');
        },
        unhighlight: function (element) {
            var id_attr = &quot;#&quot; + $(element).attr(&quot;id&quot;) + &quot;1&quot;;
            $(element).closest('.form-group').removeClass('has-feedback error').addClass('has-feedback success');
            $(id_attr).removeClass('glyphicon-remove').addClass('glyphicon-ok');
        },
        errorContainer: '#errorBox',
        errorLabelContainer: $(&quot;#errorBox ul&quot;),
        errorElement: 'li',
    });

    $('input[name=gender]').on('change', function () {
        $('#newUserForm').valid();
    });

    $(&quot;#submit&quot;).click(function (e) {
        e.preventDefault();
        $('#newUserForm').submit();
    });
});
&lt;/script&gt;
