            &lt;/p&gt;
            &lt;a href=&quot;{{ link.pages-managed-dashboard-subscribed }}&quot; class=&quot;btn btn-primary&quot;&gt;{{page_action_btn}}&lt;/a&gt;
        &lt;/div&gt;
    &lt;/section&gt;
