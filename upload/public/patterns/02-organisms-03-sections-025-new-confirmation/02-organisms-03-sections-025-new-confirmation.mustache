            &lt;div class=&quot;glyphicon glyphicon-ok success glyphicon--super-circle&quot; aria-hidden=&quot;true&quot;&gt;&lt;/div&gt;
            &lt;h1&gt;{{page_title}}&lt;/h1&gt;
