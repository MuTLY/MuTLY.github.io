&lt;div class=&quot;row-margin-fix loans-summary-tbl js-eq-ht-container&quot; data-breakpt=&quot;768&quot;&gt;
	&lt;div class=&quot;block block-well col-sm-6 col-lg-6 block-left border-right-collapse js-eq-ht-div&quot;&gt;
		&lt;div class=&quot;well-title loans-summary-tbl--mbl__heading&quot;&gt;Total Loans: {{loansCount}}&lt;/div&gt;
		&lt;div class=&quot;well-body&quot;&gt;
			&lt;p class=&quot;text-center&quot;&gt;The following loans are outstanding as of {{outstandingDate}}&lt;/p&gt;
            &lt;div class=&quot;loans-summary-tbl--mbl__body&quot;&gt;
                &lt;table class=&quot;tbl-noborder tbl-nobkg&quot;&gt;
                    {{# loanDetails }}
                    &lt;tr&gt;
                        &lt;th id=&quot;loan_{{ index }}&quot; scope=&quot;row&quot;&gt;Loan {{ index }}:&lt;/th&gt;
                        &lt;td class=&quot;text-right&quot; headers=&quot;loan_{{ index }}&quot;&gt;{{loan_id}}&lt;/td&gt;
                        &lt;td class=&quot;text-right&quot; headers=&quot;loan_{{ index }}&quot;&gt;{{col_1_row_1_data_text}}&lt;/td&gt;
                    &lt;/tr&gt;
                    {{/ loanDetails }}
                    &lt;tfoot&gt;
                        &lt;tr class=&quot;row-border-top&quot;&gt;
                            &lt;th colspan=&quot;2&quot; class=&quot;text-right&quot; id=&quot;summary_total&quot; headers=&quot;loan_0 loan_1&quot;&gt;Total&lt;/th&gt;
                            &lt;th class=&quot;text-right&quot; headers=&quot;loan_0 loan_1 summary_total&quot;&gt;$8,916.55&lt;/th&gt;
                        &lt;/tr&gt;
                    &lt;/tfoot&gt;
                &lt;/table&gt;
                &lt;hr class=&quot;visible-xs&quot; /&gt;
            &lt;/div&gt;
		&lt;/div&gt;
	&lt;/div&gt;

	&lt;div class=&quot;block block-well col-sm-6 col-lg-6 block-right loans-summary-tbl--mbl__bottom js-eq-ht-div&quot;&gt;
		&lt;div class=&quot;well-title hidden-xs&quot;&gt;Payoff Details&lt;/div&gt;
		&lt;div class=&quot;well-body text-center-sm&quot;&gt;
			&lt;p&gt;Select &lt;strong&gt;Payoff Amount Details&lt;/strong&gt; to view loan details and instructions. If available, select &lt;strong&gt;Amortization Schedule&lt;/strong&gt; or &lt;strong&gt;Loan Note Security Agreement&lt;/strong&gt; to view those documents.&lt;/p&gt;
			&lt;p&gt;For more information on your loan details or documents, please contact us: &lt;strong class=&quot;hidden-xs&quot;&gt;1-800-755-5801&lt;/strong&gt;&lt;/p&gt;
            &lt;a href=&quot;#nav_contact&quot; class=&quot;btn btn-primary hidden-sm hidden-md hidden-lg&quot;&gt;
                &lt;span class=&quot;trs-icons-phone&quot; aria-hidden=&quot;true&quot;&gt;&lt;/span&gt;
                1-800-755-5801
            &lt;/a&gt;
		&lt;/div&gt;
	&lt;/div&gt;

    &lt;div class=&quot;clear&quot;&gt;&lt;/div&gt;
&lt;/div&gt;

&lt;div class=&quot;row padding-bottom&quot;&gt;
	&lt;div class=&quot;col-xs-12&quot;&gt;
		{{# loanDetails }}
			&lt;div class=&quot;hidden-xs loans-payoff__title&quot;&gt;
				Loan Number: {{loan_id}}
				(Payoff Amount: {{col_1_row_1_data_text}})
			&lt;/div&gt;
			{{&gt; molecules-table-loan-payoff }}
		{{/ loanDetails }}
	&lt;/div&gt;
&lt;/div&gt;
