

&lt;div class=&quot;panel-group trs-accordion-table {{{class-trs-details}}}&quot; id=&quot;{{accordion_id}}&quot;&gt;
{{#accordion_items }}
  &lt;div class=&quot;panel panel-default first&quot;&gt;
    &lt;div class=&quot;accordion-toggle panel-heading&quot; data-toggle=&quot;collapse&quot; data-parent=&quot;#{{accordion_id}}&quot; href=&quot;#{{id}}&quot;&gt;
      
        &lt;div class=&quot;row&quot;&gt;
        {{# heading}}
          &lt;div class=&quot;col-sm-3 {{{class}}}&quot;&gt;{{{ value }}}&lt;/div&gt;
         {{/ heading}}

		 {{# headingone}}
          &lt;div class=&quot;col-sm-2 {{{class}}}&quot;&gt;{{{ value }}}&lt;/div&gt;
		  &lt;div class=&quot;col-sm-1&quot;&gt;{{# icon }}&lt;i class=&quot;{{glyph_icon}}&quot; aria-hidden=&quot;true&quot;&gt; &lt;/i&gt;{{/ icon }}&lt;/div&gt;
         {{/ headingone}}

        &lt;/div&gt;
      
    &lt;/div&gt;
    &lt;div id=&quot;{{id}}&quot; class=&quot;panel-collapse collapse  {{state}}&quot;&gt;
      &lt;div class=&quot;panel-body&quot;&gt;
      {{# rows}}
        &lt;div class=&quot;row&quot;&gt;
        {{# cols }}
          &lt;div class=&quot;col-sm-3 {{{ col-class }}}&quot;&gt;{{{ value }}}&lt;/div&gt;
        {{/ cols}}
		{{# colsone }}
          &lt;div class=&quot;col-sm-2 {{{ col-class }}}&quot;&gt;{{{ value }}}&lt;/div&gt;		 
        {{/ colsone}}

		{{# colstwo }}
          &lt;div class=&quot;col-sm-1 {{{ col-class }}}&quot;&gt;{{{ value }}}&lt;/div&gt;
        {{/ colstwo}}
        &lt;/div&gt;
      {{/ rows}}
      &lt;/div&gt;
    &lt;/div&gt;
  &lt;/div&gt;
{{/accordion_items }}
&lt;/div&gt;
